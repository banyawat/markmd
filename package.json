{
  "name": "markmd",
  "version": "0.1.0-rc7",
  "description": "Node.JS tools for translate markdown to beautiful API document",
  "main": "/build/index.js",
  "repository": "git@github.com:banyawat/markmd.git",
  "author": "THiNKNET CNX",
  "license": "MIT",
  "private": false,
  "bin": {
    "markmd": "./build/index.js"
  },
  "scripts": {
    "convert": "babel-node index.js",
    "dev": "nodemon --watch index.js --watch src --exec \"babel-node index\" -e js,css",
    "start": "node build/index.js",
    "build-css": "node-minify --compressor clean-css --input 'src/styles/index.css' --output 'build/src/styles/index.css'",
    "build": "babel index.js -d build; babel src -d build/src; npm run build-css;",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.4",
    "fs-extra": "8.0.1",
    "merge-deep": "3.0.2",
    "mkdirp": "0.5.1",
    "showdown": "1.9.0",
    "smart-fs": "1.7.6"
  },
  "devDependencies": {
    "@node-minify/cli": "4.1.0",
    "@node-minify/clean-css": "4.0.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "nodemon": "1.19.0"
  }
}
